@model List<GymManager.Core.MembershipTypes.MembershipType>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Cost</th>
      <th scope="col">CreatedOn</th>
      <th scope="col">Duration </th>
      <th scope="col"></th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    @foreach(var member in Model)
        {
            <tr>
                <th scope="row">@member.Name</th>
                <td>@member.Cost</td>
                <td>@member.CreatedOn.ToString("dd/MM/yyyy")</td>
                <td>@member.Duration</td>
            </tr>
        }
    </tbody>
</table>